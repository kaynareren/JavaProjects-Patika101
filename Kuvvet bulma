package giris;

import java.util.Scanner;

public class Baslangic {
    public static void main(String[] args) {

    int a , i , j;

    Scanner eren = new Scanner(System.in);

        System.out.print("Bir Sayi Giriniz : ");
        a = eren.nextInt();

        for (i = 1; i <= a; i *= 4) {
            System.out.println(i);
        }
        System.out.println("---------------");

        for (j = 1; j <= a; j *= 5) {
            System.out.println(j);
        }
    }
}
