package giris;

import java.util.Scanner;

public class Baslangic {
    public static void main(String[] args) {

        Scanner eren = new Scanner(System.in);

        for (int i = 2; i <= 100; i++) {
            boolean prime = true;
            for (int j = 2; j < i; j++) {
                if (i % j == 0) {
                    prime = false;
                }
            }
            if (prime) {
                System.out.println(i + " ");
            }
        }
    
